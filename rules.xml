<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 https://www.mojohaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <!-- placeholder for future use -->
    <!--
        <ignoreVersions>
            <ignoreVersion type="regex">.*-beta</ignoreVersion>
        </ignoreVersions>
    -->

    <rules>
        <rule groupId="com.patientkeeper" artifactId="mobilizer" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">.*-f.*</ignoreVersion>
                <ignoreVersion type="regex">.*-b.*</ignoreVersion>
                <ignoreVersion type="regex">.*-s.*</ignoreVersion>
                <ignoreVersion type="regex">.*-d.*</ignoreVersion>
                <ignoreVersion type="regex">.*-h.*</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>